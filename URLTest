public class Stopover{

    public static boolean paraFunction(String url){

        String prefix = "";
        if(url.startsWith("http://")){
            prefix = "http://";
        } else if(url.startsWith("https://")){
            prefix = "https://";
        } else if(url.startsWith("www.")){
            prefix = "www.";
        } else{
            return false;
        }

        String remaining = url.substring(prefix.length());

        String[] fields = remaining.split("\\.");

        for(String field : fields){
            if(field.length() <= 2){
                return false;
            }
            for(int i = 0; i<field.length(); i++){
                char ch = field.charAt(i);

                if(!Character.isLetterOrDigit(ch)){
                    return false;
                }
            }
        }
         return true;
    }
    public static void main(String[] args) 
    {
        System.out.println(paraFunction("http://google.com"));
        System.out.println(paraFunction("https://w3Schools.com"));
        System.out.println(paraFunction("ww.wikipedia.com"));
    }
}
